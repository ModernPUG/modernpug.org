{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ext-dom": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "creativeorange/gravatar": "^1.0",
        "doctrine/dbal": "^2.8",
        "embed/embed": "^3.3",
        "fideloper/proxy": "^4.0",
        "gpressutto5/laravel-slack": "^1.1",
        "guzzlehttp/guzzle": "^6.3",
        "jolicode/slack-php-api": "^1.2",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.5.1",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/socialite": "^4.1",
        "laravel/tinker": "^1.0",
        "oriceon/toastr-5-laravel": "dev-master",
        "sentry/sentry-laravel": "^1.1",
        "socialiteproviders/slack": "^3.0",
        "spatie/laravel-permission": "^2.37",
        "spatie/laravel-slack-slash-command": "^1.8",
        "symfony/dom-crawler": "^4.3",
        "zendframework/zend-feed": "^2.12",
        "zendframework/zend-http": "^2.10"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.2",
        "codedungeon/phpunit-result-printer": "^0.23.4",
        "filp/whoops": "^2.4",
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "^1.2",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "test": [
            "@php ./vendor/bin/phpunit"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
