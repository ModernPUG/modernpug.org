{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "creativeorange/gravatar": "^1.0",
        "doctrine/dbal": "^3.1",
        "embed/embed": "^4.3",
        "fruitcake/laravel-cors": "^2.0.5",
        "guzzlehttp/guzzle": "^7.3",
        "laminas/laminas-feed": "^2.14",
        "laminas/laminas-http": "^2.14",
        "laravel/framework": "^9.1",
        "laravel/sanctum": "^2.11",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.3",
        "sentry/sentry-laravel": "^2.6",
        "socialiteproviders/discord": "^4.1",
        "socialiteproviders/slack": "^4.1",
        "spatie/laravel-flash": "^1.8",
        "spatie/laravel-permission": "^5.5",
        "symfony/dom-crawler": "^5.3",
        "yoeunes/toastr": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "fakerphp/faker": "^1.14",
        "nunomaduro/collision": "^6.1",
        "itsgoingd/clockwork": "^5.0",
        "mockery/mockery": "^1.4",
        "spatie/laravel-ignition": "^1.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "test": [
            "@php artisan optimize --env=testing",
            "@php artisan db:wipe --env=testing",
            "@php artisan migrate --seed --env=testing",
            "@php artisan test --env=testing",
            "@php artisan config:clear --env=testing"
        ],
        "rebuild": [
            "@php artisan migrate:fresh --ansi",
            "@php artisan db:seed --ansi",
            "@php artisan ide-helper:model -WR --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
