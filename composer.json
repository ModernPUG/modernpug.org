{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mcrypt": "*",
        "ext-pdo": "*",
        "creativeorange/gravatar": "^1.0",
        "doctrine/dbal": "^2.8",
        "embed/embed": "^3.3",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "jolicode/slack-php-api": "^1.2",
        "laravel/framework": "^7.19",
        "laravel/passport": "^8.0",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/socialite": "^4.1",
        "laravel/tinker": "^2.4",
        "laravel/ui": "^2.1",
        "oriceon/toastr-5-laravel": "dev-master",
        "sentry/sentry-laravel": "1.8.0",
        "socialiteproviders/slack": "^3.1",
        "spatie/laravel-permission": "^3.13",
        "spatie/laravel-slack-slash-command": "^1.10",
        "symfony/dom-crawler": "^4.3",
        "zendframework/zend-feed": "^2.12",
        "zendframework/zend-http": "^2.10"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "beyondcode/laravel-dump-server": "^1.4",
        "codedungeon/phpunit-result-printer": "^0.28.0",
        "facade/ignition": "^2.0",
        "filp/whoops": "^2.4",
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^1.2",
        "nunomaduro/collision": "^4.2",
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "test": [
            "@php artisan config:cache --env=testing",
            "@php artisan migrate --seed --env=testing",
            "./vendor/bin/phpunit",
            "@php artisan config:clear --env=testing"
        ],
        "rebuild": [
            "@php artisan migrate:fresh --ansi",
            "@php artisan db:seed --ansi",
            "@php artisan feed:crawl --ansi",
            "@php artisan release-news:crawl --ansi",
            "@php artisan image:update --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
