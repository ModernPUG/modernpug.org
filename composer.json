{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mcrypt": "*",
        "ext-pdo": "*",
        "creativeorange/gravatar": "^1.0",
        "doctrine/dbal": "^2.8",
        "embed/embed": "^3.3",
        "facade/ignition": "^2.3",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/framework": "^8.3",
        "laravel/sanctum": "^2.8",
        "laravel/slack-notification-channel": "^2.2",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.4",
        "laravel/ui": "^3.0",
        "nunomaduro/collision": "^5.0",
        "oriceon/toastr-5-laravel": "dev-master",
        "phpunit/phpunit": "^9.3",
        "sentry/sentry-laravel": "^1.9",
        "socialiteproviders/slack": "^3.1",
        "spatie/laravel-flash": "^1.8",
        "spatie/laravel-permission": "^3.13",
        "symfony/dom-crawler": "^4.3",
        "zendframework/zend-feed": "^2.12",
        "zendframework/zend-http": "^2.10"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "beyondcode/laravel-dump-server": "^1.4",
        "codedungeon/phpunit-result-printer": "^0.28.0",
        "filp/whoops": "^2.4",
        "fzaninotto/faker": "^1.9",
        "itsgoingd/clockwork": "^5.0",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/Factories/",
            "Database\\Seeders\\": "database/Seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "test": [
            "@php artisan optimize --env=testing",
            "@php artisan db:wipe --env=testing",
            "@php artisan migrate --seed --env=testing",
            "@php artisan test --env=testing",
            "@php artisan config:clear --env=testing"
        ],
        "rebuild": [
            "@php artisan migrate:fresh --ansi",
            "@php artisan db:seed --ansi",
            "@php artisan ide-helper:model -WR --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
